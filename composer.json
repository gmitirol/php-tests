{
    "name": "gmi/php-tests",
    "license": "proprietary",
    "type": "library",
    "description": "PHP Test Suite",
    "authors": [
        {
        "name": "Nikola Vrlazic",
        "email": "nikola.vrlazic@i-med.ac.at"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6"
    },
    "suggest": {
        "ext-mbstring": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-date": "*",
        "ext-calendar": "*",
        "ext-iconv": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Gmi\\PhpTests\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gmi\\PhpTests\\Tests\\": "tests"
        }
    }
}
