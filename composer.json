{
    "name": "gmi/php-tests",
    "license": "LGPL-3.0-only",
    "type": "library",
    "description": "PHP Test Suite",
    "authors": [
        {
        "name": "Nikola Vrlazic",
        "email": "nikola.vrlazic@i-med.ac.at"
        },
        {
        "name": "Andreas Erhard",
        "email": "andreas.erhard@i-med.ac.at"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6"
    },
    "suggest": {
        "ext-PDO": "*",
        "ext-Reflection": "*",
        "ext-SPL": "*",
        "ext-SimpleXML": "*",
        "ext-bcmath": "*",
        "ext-bz2": "*",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-gettext": "*",
        "ext-gmp": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-imap": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-memcached": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-readline": "*",
        "ext-session": "*",
        "ext-shmop": "*",
        "ext-sockets": "*",
        "ext-tokenizer": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "ext-zlib": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Gmi\\PhpTests\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gmi\\PhpTests\\Tests\\": "tests"
        }
    }
}
